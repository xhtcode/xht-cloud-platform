    <!-- 通用查询结果列 -->
    <sql id="BaseColumnList">
        id,
        gen_db_id,
        table_schema,
        table_engine,
        table_name,
        module_label,
        module_value,
        path_url,
        code_name,
        gen_type,
        menu_id,
        table_create_time,
        table_update_time,
        create_by,
        update_by,
        create_time,
        update_time
