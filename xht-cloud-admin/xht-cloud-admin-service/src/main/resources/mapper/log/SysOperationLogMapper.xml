        <result column="id" property="id" />
        <result column="server_name" property="serverName" />
        <result column="bus_name" property="busName" />
        <result column="bus_desc" property="busDesc" />
        <result column="operate_method" property="operateMethod" />
        <result column="operate_type" property="operateType" />
        <result column="operate_name" property="operateName" />
        <result column="request_url" property="requestUrl" />
        <result column="operate_ip" property="operateIp" />
        <result column="operate_param" property="operateParam" />
        <result column="status" property="status" />
        <result column="error_msg" property="errorMsg" />
        <result column="create_time" property="createTime" />
    </resultMap>


    <!-- 通用查询结果列 -->
    <sql id="BaseColumnList">
        id,
        server_name,
        bus_name,
        bus_desc,
        operate_method,
        operate_type,
        operate_name,
        request_url,
        operate_ip,
        operate_location,
